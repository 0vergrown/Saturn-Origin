{
   "name":"§6§nUnlimited Void",
   "description":"The Unlimited Void presents the target with limitless information, forcing them to see and feel everything while seeing and feeling nothing at the exact same time. This phenomenon paralyzes the target with infinite knowledge, causing them to die slowly. This domain heightens your abilities while active, after 30 seconds the target will be sent out of the domain however the caster can stay and leave as they please. (Slowness X, Blindness X, and Wither I to target for 15 seconds, Caster gains Strength I while in Unlimited Void.) §f[Save Toolbar Activator]",
   "type":"origins:multiple",
   "resource":{
      "type":"origins:resource",
      "min":0,
      "max":1,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "set":{
      "type":"origins:entity_set",
      "action_on_remove":{
         "type":"origins:target_action",
         "action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:execute_command",
                  "command":"forceload remove ~ ~ ~ ~"
               },
               {
                  "type":"origins:delay",
                  "action":{
                     "type":"origins:execute_command",
                     "command":"kill @s"
                  },
                  "ticks":4
               }
            ]
         }
      }
   },
   "active":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:resource",
            "resource":"saturn:unlimited_void_resource",
            "comparison":"==",
            "compare_to":1
         },
         "if_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:change_resource",
                  "resource":"saturn:unlimited_void_resource",
                  "change":0,
                  "operation":"set"
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.power_select",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.deactivate",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.respawn_anchor.charge",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:action_on_entity_set",
                  "set":"saturn:unlimited_void_set",
                  "bientity_action":{
                     "type":"origins:and",
                     "actions":[
                        {
                           "type":"origins:actor_action",
                           "action":{
                              "type":"origins:and",
                              "actions":[
                                 {
                                    "type":"origins:execute_command",
                                    "command":"tag @s add saturn.unlimited_void_actor"
                                 },
                                 {
                                    "type":"origins:delay",
                                    "action":{
                                       "type":"origins:execute_command",
                                       "command":"tag @s remove saturn.unlimited_void_actor"
                                    },
                                    "ticks":2
                                 }
                              ]
                           }
                        },
                        {
                           "type":"origins:target_action",
                           "action":{
                              "type":"origins:delay",
                              "action":{
                                 "type":"origins:execute_command",
                                 "command":"tp @e[tag=saturn.unlimited_void_actor,limit=1] @s"
                              },
                              "ticks":1
                           }
                        },
                        {
                           "type":"origins:delay",
                           "action":{
                              "type":"origins:remove_from_entity_set",
                              "set":"saturn:unlimited_void_set"
                           },
                           "ticks":3
                        }
                     ]
                  },
                  "limit":1,
                  "reverse":false
               }
            ]
         },
         "else_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:spawn_entity",
                  "entity_type":"minecraft:armor_stand",
                  "tag":"{NoGravity:1b,Invulnerable:1b,Small:1b,Marker:1b,Invisible:1b,DisabledSlots:4144959}",
                  "entity_action":{
                     "type":"origins:execute_command",
                     "command":"forceload add ~ ~ ~ ~"
                  },
                  "bientity_action":{
                     "type":"origins:add_to_entity_set",
                     "set":"saturn:unlimited_void_set"
                  }
               },
               {
                  "type":"origins:raycast",
                  "distance":3.00,
                  "block":false,
                  "entity":true,
                  "shape_type":"visual",
                  "fluid_handling":"none",
                  "bientity_condition":{
                     "type":"origins:target_condition",
                     "condition":{
                        "type":"origins:living"
                     }
                  },
                  "bientity_action":{
                     "type":"origins:target_action",
                     "action":{
                        "type":"origins:and",
                        "actions":[
                           {
                              "type":"origins:grant_power",
                              "power":"saturn:unlimited_void_mob_effects",
                              "source":"saturn:unlimited_void_mob_effects"
                           },
                           {
                              "type":"origins:delay",
                              "action":{
                                 "type":"origins:execute_command",
                                 "command":"execute in saturn:unlimited_void positioned as @s run teleport ~0.5 3 8.5"
                              },
                              "ticks":1
                           }
                        ]
                     }
                  },
                  "command_step":1.00,
                  "command_along_ray_only_on_hit":false
               },
               {
                  "type":"origins:change_resource",
                  "resource":"saturn:unlimited_void_resource",
                  "change":1,
                  "operation":"set"
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.power_select",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.deactivate",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.respawn_anchor.charge",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:delay",
                  "action":{
                     "type":"origins:execute_command",
                     "command":"execute in saturn:unlimited_void positioned as @s run teleport ~0.5 3 8.5"
                  },
                  "ticks":1
               }
            ]
         }
      },
      "cooldown":300,
      "hud_render":{
         "should_render":true,
         "sprite_location":"origins:textures/gui/resource_bar.png",
         "bar_index":0
      },
      "key":{
         "key":"key.saveToolbarActivator",
         "continuous":false
      }
   },
   "remove_gates_on_death":{
      "type":"origins:action_on_death",
      "bientity_action":{
         "type":"origins:target_action",
         "action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:change_resource",
                  "resource":"saturn:unlimited_void_resource",
                  "change":0,
                  "operation":"set"
               },
               {
                  "type":"origins:action_on_entity_set",
                  "set":"saturn:unlimited_void_set",
                  "bientity_action":{
                     "type":"origins:remove_from_entity_set",
                     "set":"saturn:unlimited_void_set"
                  },
                  "limit":1,
                  "reverse":false
               }
            ]
         }
      },
      "bientity_condition":{
         "type":"origins:target_condition",
         "condition":{
            "type":"origins:resource",
            "resource":"saturn:unlimited_void_resource",
            "comparison":"==",
            "compare_to":1
         }
      }
   },
   "in_unlimited_void_effects":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:spawn_particles",
               "particle":"minecraft:enchant",
               "count":40,
               "speed":1.00,
               "force":true,
               "spread":{
                  "x":8.00,
                  "y":8.00,
                  "z":8.00
               },
               "offset_x": 0.00,
               "offset_y":0.50,
               "offset_z": 0.00
            },
            {
               "type":"origins:apply_effect",
               "effect":{
                  "id":"minecraft:strength",
                  "duration":100,
                  "amplifier":0,
                  "ambient":true,
                  "show_particles":false,
                  "show_icon":true
               }
            }
         ]
      },
      "falling_action":{
         "type":"origins:clear_effect",
         "effect":"minecraft:strength"
      },
      "interval":1,
      "condition":{
         "type":"origins:dimension",
         "dimension":"saturn:unlimited_void",
         "inverted":false
      }
   }
}