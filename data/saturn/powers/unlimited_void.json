{
   "name":"§6§nUnlimited Void",
   "description":"The Unlimited Void presents the target with limitless information, forcing them to see and feel everything while seeing and feeling nothing at the exact same time. This phenomenon paralyzes the target with infinite knowledge, causing them to die slowly. This domain heightens your abilities while active, after 30 seconds the target will be sent out of the domain however the caster can stay and leave as they please. (Slowness X, Blindness X, and Wither I to target for 15 seconds, Caster gains Strength I while in Unlimited Void.) §f[Save Toolbar Activator]",
   "type":"origins:multiple",
   "give_id":{
      "type":"origins:action_on_callback",
      "entity_action_added":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:execute_command",
               "command":"scoreboard players add #saturn_fakeplayer saturn.unlimited_void_id 1"
            },
            {
               "type":"origins:execute_command",
               "command":"scoreboard players operation @s saturn.unlimited_void_id = #saturn_fakeplayer saturn.unlimited_void_id"
            }
         ]
      },
      "execute_chosen_when_orb":true
   },
   "active":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:and",
            "conditions":[
               {
                  "type":"origins:dimension",
                  "dimension":"saturn:unlimited_void",
                  "inverted":false
               },
               {
                  "type":"origins:command",
                  "command":"function saturn:unlimited_void_checkid",
                  "compare_to":0,
                  "comparison":">"
               },
               {
                  "type":"origins:nbt",
                  "nbt":"{Tags:[\"saturn.unlimited_voidExists\"]}"
               }
            ]
         },
         "if_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:execute_command",
                  "command":"function saturn:unlimited_void_leave"
               },
               {
                  "type":"origins:change_resource",
                  "resource":"saturn:unlimited_void_resource",
                  "change":0,
                  "operation":"set"
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.power_select",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.deactivate",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.respawn_anchor.charge",
                  "volume":1.00,
                  "pitch":1.00
               }
            ]
         },
         "else_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:spawn_entity",
                  "entity_type":"minecraft:armor_stand",
                  "tag":"{Invisible:1b,Invulnerable:1b,NoGravity:1b,Small:1b,Marker:1b,Tags:[\"saturn.unlimited_void_active\",\"saturn.unlimited_void_new\"],DisabledSlots:4144959}"
               },
               {
                  "type":"origins:change_resource",
                  "resource":"saturn:unlimited_void_resource",
                  "change":1,
                  "operation":"add"
               },
               {
                  "type":"origins:raycast",
                  "distance":16,
                  "block":false,
                  "entity":true,
                  "shape_type":"collider",
                  "fluid_handling":"none",
                  "space":"local",
                  "bientity_condition":{
                     "type":"origins:target_condition",
                     "condition":{
                        "type":"origins:living"
                     }
                  },
                  "bientity_action":{
                     "type":"origins:target_action",
                     "action":{
                        "type":"origins:and",
                        "actions":[
                           {
                              "type":"origins:grant_power",
                              "power":"saturn:unlimited_void_mob_effects",
                              "source":"saturn:unlimited_void_mob_effects"
                           },
                           {
                              "type":"origins:execute_command",
                              "command":"scoreboard players add #saturn_visitor_fakeplayer saturn.unlimited_void_visitor_id 1"
                           },
                           {
                              "type":"origins:execute_command",
                              "command":"scoreboard players operation @s saturn.unlimited_void_visitor_id = #saturn_visitor_fakeplayer saturn.unlimited_void_visitor_id"
                           },
                           {
                              "type":"origins:spawn_entity",
                              "entity_type":"minecraft:armor_stand",
                              "tag":"{Invisible:1b,Invulnerable:1b,NoGravity:1b,Small:1b,Marker:1b,Tags:[\"saturn.unlimited_void_visitor_active\",\"saturn.unlimited_void_visitor_new\"],DisabledSlots:4144959}"
                           },
                           {
                              "type":"origins:execute_command",
                              "command":"function saturn:unlimited_void_visitor_enter"
                           }
                        ]
                     }
                  },
                  "entity_distance":16.0,
                  "command_step":1.0,
                  "command_along_ray_only_on_hit":false
               },
               {
                  "type":"origins:execute_command",
                  "command":"function saturn:unlimited_void_enter"
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.power_select",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.beacon.deactivate",
                  "volume":1.00,
                  "pitch":1.00
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:block.respawn_anchor.charge",
                  "volume":1.00,
                  "pitch":1.00
               }
            ]
         }
      },
      "cooldown":300,
      "hud_render":{
         "should_render":true,
         "sprite_location":"origins:textures/gui/resource_bar.png",
         "bar_index":0
      },
      "key":{
         "key":"key.saveToolbarActivator",
         "continuous":false
      }
   },
   "in_unlimited_void_effects":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:spawn_particles",
               "particle":"minecraft:enchant",
               "count":40,
               "speed":1.0,
               "force":true,
               "spread":{
                  "x":8.0,
                  "y":8.0,
                  "z":8.0
               },
               "offset_y":0.5
            },
            {
               "type":"origins:apply_effect",
               "effect":{
                  "effect":"minecraft:strength",
                  "duration":100,
                  "amplifier":0,
                  "is_ambient":true,
                  "show_particles":false,
                  "show_icon":true
               }
            }
         ]
      },
      "falling_action":{
         "type":"origins:clear_effect",
         "effect":"minecraft:strength"
      },
      "interval":1,
      "condition":{
         "type":"origins:dimension",
         "dimension":"saturn:unlimited_void",
         "inverted":false
      }
   }
}